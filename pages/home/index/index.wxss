.content {
  height: 100%;
  display: flex;
  flex-direction: column;
  /*justify-content: space-between;*/
  flex: 1;
}

.indexSwiper {
  width: 100%;
  height: 380rpx;
}

.indexSwiper image {
  width: 100%;
  height: 100%;
}

.indexMiddle {
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  justify-content: center;
  width: 100%;
  background-color: #fbf9fe;
  padding: 10rpx 0;
}

.symnolView {
  width: 173rpx;
  text-align: center;
  margin: 5rpx 5rpx;
  padding: 5rpx 0;
  background: #fff;
  border-radius: 10rpx;
}

.symnolView image {
  width: 58rpx;
  height: 58rpx;
  margin-bottom: -2rpx
}

.symnolView text {
  display: block;
  font-size: 20rpx;
  color: #929292;
  margin:5rpx 0
}

.schoolpush {
  margin: 20rpx 30rpx;
  font-size: 30rpx;
}

.bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 5rpx 0;
  border-bottom: 0.5rpx solid #eee;
  height: 80rpx;
  font-size: 30rpx;
  padding: 0 50rpx;
  background-color: #fff
}

.bar .noticeTitle {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.bar .noticeTime {
  font-size: 25rpx;
  color: rgb(163, 163, 163);
}

.viewimg {
  width: 45rpx;
  height: 44rpx;
  padding: 18rpx 14rpx;
  margin: 0 0 0 580rpx;
  position: absolute;
}

view {
  font-family: "微软雅黑";
}

.scroll {
  height: 400rpx;
  background:#fbf9fe;
}

.title {
  display: flex;
  align-items: center;
  height: 75rpx;
  font-size: 30rpx;
}

.title .noticeTitle {
  width: 33%;
  display: flex;
  justify-content: center;
  border-top: 15rpx solid rgba(153, 204, 255, 0.5);
  height: 60rpx;
  align-items: center;
  margin: 0 2rpx;
  background: #fbf9fe;
  color: #929292;
}

.title .active {
  background-color: #fff;
  border-top: 15rpx solid rgb(153, 204, 255);
    color: #000;
}
